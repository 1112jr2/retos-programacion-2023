#include<stdio.h>



int main()
{
    for(int i = 1; i < 100; i++)
    {
        if(i % 3 == 0)
        {
            printf("fizz = %i\n", i);
        }
        else if(i % 5 == 0)
        {
            printf("buzz = %i\n", i);
        }
        else if(i % 3 == 0 || i % 5  == 0)
        {
            printf("fizzbuzz %i\n", i);
        }
        else
        {
            printf("%i\n", i);
        }
    }
    return 0;
}

